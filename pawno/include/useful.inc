ScrambleWord(s[])
{
    new new_s[64];
    new tmp[2], num, len = strlen(s);

    for(new i=0; s[i] != EOS; i++)
    {
        num = random(len);
        tmp[0] = s[i];
        tmp[1] = s[num];
        s[num] = tmp[0];
        s[i] = tmp[1];
   }

    strins(new_s, s, 0);

    return new_s;
}

