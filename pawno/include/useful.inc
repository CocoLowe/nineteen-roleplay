stock ArraySort(array[], size)
{
    new i, j, app;
        
    for(i = 1; i < size; i++)
    {
        app = array[i];

        for(j = i - 1; (j >= 0) && (array[j] > app); j--)
        {
            array[j + 1 ] = array[j];
       }
        
        array[j + 1] = app;
   }

    return 1;
}

